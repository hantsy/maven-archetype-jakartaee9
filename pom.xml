<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>io.github.hantsy</groupId>
  <artifactId>maven-archetype-jakartaee9</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>maven-archetype</packaging>

  <name>maven-archetype-jakartaee9</name>
  <description>Archetype - Maven archetype for Jakarta EE 9</description>
  <url>https://github.com/hantsy/maven-archetype-jakartaee9</url>


  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
    <archetype-packaging.version>3.2.0</archetype-packaging.version>
  </properties>

  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.archetype</groupId>
        <artifactId>archetype-packaging</artifactId>
        <version>${archetype-packaging.version}</version>
      </extension>
    </extensions>
  </build>
  <issueManagement>
    <system>Github Issues</system>
    <url>https://github.com/hantsy/maven-archetype-jakartaee9/issues</url>
  </issueManagement>
  <ciManagement>
    <system>Github Actions</system>
    <url>https://github.com/hantsy/maven-archetype-jakartaee9/actions</url>
  </ciManagement>
  <scm>
    <url>https://github.com/hantsy/maven-archetype-jakartaee9</url>
    <connection>scm:git:https://github.com/hantsy/maven-archetype-jakartaee9</connection>
    <developerConnection>scm:git:https://github.com/hantsy/maven-archetype-jakartaee9</developerConnection>
  </scm>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>Hantsy Bai</name>
      <email>hantsy@gmail.com</email>
      <timezone>UTC+8</timezone>
      <url>https://github.com/hantsy/</url>
    </developer>
  </developers>
</project>
